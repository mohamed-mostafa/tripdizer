  <body ng-app="G2GControlCenterApplication" class="skin-blue">
    <div class="wrapper">
      <!-- Contains the navbar and content of the page -->
      <div class="content-wrapper" style="margin: 0px" ng-controller="ConfigurationsPageContentController">
        <!-- Content Header (Page header) -->

        <!-- Main content -->
        <section class="content">
          <!-- Small boxes (Stat box) -->
          <div class="box">
                <div class="box-header">
                  <h3 class="box-title">Submit Request</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                <div class="overlay"  ng-show="loading == true">
                  <i class="fa fa-refresh fa-spin"></i>
                </div>
                <!-- UNCOMMENT THIS -->
                <div class="box-body table-responsive no-padding">
                  <table id="configurationsTable" class="table table-bordered table-striped">
                    <thead>
                      <tr>
                        <th>Question</th>
                        <th>Answer</th>
                      </tr>
                    </thead>
                    <tbody>
                      	<tr ng-repeat="questionAnswer in request.questionAnswers">
						  	<td ng-bind="questionAnswer.question.text"></td>
							<td><input type="text" class="form-control" ng-model="questionAnswer.answer.answer"></input></td>
						</tr>
                    </tbody>
                   </table>
                   <hr/>
                   <table id="configurationsTable" class="table table-bordered table-striped">
                    <tbody>
                    	<tr>
						  	<td>Your Name</td>
							<td><input type="text" class="form-control" ng-model="request.traveler.name"></input></td>
						</tr>
						<tr>
						  	<td>Your Email</td>
							<td><input type="text" class="form-control" ng-model="request.traveler.emailAddress"></input></td>
						</tr>
						<tr>
						  	<td>Your Mobile</td>
							<td><input type="text" class="form-control" ng-model="request.traveler.mobile"></input></td>
						</tr>
						<tr>
						  	<td>Your Date of birth</td>
							<td><input type="text" class="form-control" ng-model="request.traveler.dateOfBirth"></input></td>
						</tr>
                    </tbody>
                   </table>
                   <button type="button" class="btn btn-primary" ng-click="submitRequest()">Submit</button>
                   </div>
                </div><!-- /.box-body -->
              </div><!-- /.box -->
          <!-- Main row -->
          <div class="row">
          </div><!-- /.row (main row) -->
		</div>
        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
    </div><!-- ./wrapper -->

