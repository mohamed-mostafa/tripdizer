                        </div>
                        <!-- /.box-body -->
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary" ng-click="addItinerary(true)" ng-show="editMode==false">Save and close</button>
                    <button type="button" class="btn btn-primary" ng-click="addItinerary(false)" ng-show="editMode==false">Save and add another</button>
                    <button type="button" class="btn btn-primary" ng-click="editItinerary()" ng-show="editMode==true">Save</button>
                </div>
            </div>
        </div>
    </div>
