<body ng-app="G2GControlCenterApplication" class="skin-blue">
    <div class="wrapper">

        <header class="main-header">
            <% include ../partials/header %>
        </header>

        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <% include ../partials/side %>
        </aside>

        <!-- Contains the navbar and content of the page -->
        <div class="content-wrapper" ng-controller="ItinerariesPageContentController">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    HWS Dashboard
                    <small>Itineraries</small>
                </h1>
                <ol class="breadcrumb">
                    <li>
                        <a href="home">
                            <i class="fa fa-dashboard"></i> Home</a>
                    </li>
                    <li class="active">Itineraries</li>
                </ol>
            </section>
            <!-- Main content -->
            <section class="content">
                <!-- Small boxes (Stat box) -->
                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">Itineraries</h3>
                    </div>
                    <!-- /.box-header -->
                    <div class="box-body">
                        <div class="overlay" ng-show="loading == true">
                            <i class="fa fa-refresh fa-spin"></i>
                        </div>
                        <div class="box-body table-responsive no-padding" ng-show="loading == false">
                            <table id="itinerariesTable" class="table table-bordered table-striped">
                                <thead>
                                    <tr>
                                        <th>Name</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="itinerary in itineraries">
                                        <td>
                                            <a ng-click="openDetailsDialog(itinerary.id)" href="#">{{itinerary.en_name}}</a>
                                        </td>
                                    </tr>
                                </tbody>
                                <tfooter>
                                    <tr>
                                        <td colspan="7">
                                            <button type="button" class="btn btn-primary pull-right" data-toggle="modal" data-target="addItineraryModal" ng-click="openAddDialog()">Add Itinerary</button>
                                        </td>
                                    </tr>
                                </tfooter>
                            </table>
                        </div>
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->
                <!-- Main row -->
                <div class="row">

                    <!-- MODALS -->

                    <div class="modal fade" id="addItineraryModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                    <h4 class="modal-title" id="myModalLabel">Add or Modify Itinerary</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="callout callout-danger" ng-show="serverError==true">
                                        <h4>Saving failed!</h4>
                                        <p>{{serverErrorMessage}}</p>
                                    </div>
                                    <form role="form" id="newItineraryForm">
                                        <div class="box-body">
                                            <div class="overlay" ng-show="saving == true">
                                                <i class="fa fa-refresh fa-spin"></i>
                                            </div>
                                            <div class="form-group" ng-show="saving==false">
                                                <label class="control-label text-red" for="en_name" ng-show="en_nameMissing==true">
                                                    <i class="fa fa-times-circle-o"></i>English Name is missing</label>
                                                <label for="en_name" ng-show="en_nameMissing==false">En Name</label>
                                                <input type="text" required class="form-control" id="en_name" ng-model="newItinerary.en_name" placeholder="Enter name in English"
                                                    required>
                                            </div>
                                            <div class="form-group" ng-show="saving==false">
                                                <label class="control-label text-red" for="en_description" ng-show="en_descriptionMissing==true">
                                                    <i class="fa fa-times-circle-o"></i>English Description is missing</label>
                                                <label for="en_description" ng-show="en_descriptionMissing==false">En Desc</label>
                                                <input type="text" required class="form-control" id="en_description" ng-model="newItinerary.en_description" placeholder="Enter description in English"
                                                    required>
                                            </div>
                                            <div class="form-group" ng-show="saving==false">
                                                <label class="control-label text-red" for="ar_name" ng-show="ar_nameMissing==true">
                                                    <i class="fa fa-times-circle-o"></i>Arabic Name is missing</label>
                                                <label for="ar_name" ng-show="ar_nameMissing==false">Ar Name</label>
                                                <input type="text" required class="form-control" id="ar_name" ng-model="newItinerary.ar_name" placeholder="Enter name in Arabic"
                                                    required>
                                            </div>
                                            <div class="form-group" ng-show="saving==false">
                                                <label class="control-label text-red" for="ar_description" ng-show="ar_descriptionMissing==true">
                                                    <i class="fa fa-times-circle-o"></i>Arabic Description is missing</label>
                                                <label for="ar_description" ng-show="ar_descriptionMissing==false">Ar Desc</label>
                                                <input type="text" required class="form-control" id="ar_description" ng-model="newItinerary.ar_description" placeholder="Enter description in Arabic"
                                                    required>
                                            </div>
                                            <div class="form-group" ng-show="saving==false">
                                                <label class="control-label text-red" for="dailySpendings" ng-show="dailySpendingsMissing==true">
                                                    <i class="fa fa-times-circle-o"></i>Daily Spendings is missing</label>
                                                <label for="dailySpendings" ng-show="dailySpendingsMissing==false">Daily Spendings</label>
                                                <input type="text" required class="form-control" id="dailySpendings" ng-model="newItinerary.dailySpendings" placeholder="Enter daily spendings"
                                                    required>
                                            </div>
                                            <!-- /.form group -->
                                        </div>
                                        <!-- /.box-body -->
                                    </form>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-default pull-left" data-dismiss="modal">Cancel</button>
                                    <button type="button" class="btn btn-primary" ng-click="addItinerary(true)" ng-show="editMode==false">Save and close</button>
                                    <button type="button" class="btn btn-primary" ng-click="addItinerary(false)" ng-show="editMode==false">Save and add another</button>
                                    <button type="button" class="btn btn-primary" ng-click="editItinerary()" ng-show="editMode==true">Save</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- MODALS END -->


                    <!-- MODALS -->

                    <div class="modal fade" id="detailsModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                        <div class="modal-dialog modal-lg">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                    <h4 class="modal-title" id="myModalLabel">Itinerary Details</h4>
                                </div>
                                <div class="modal-body">
                                    <div class="callout callout-danger" ng-show="serverError==true">
                                        <h4>Saving failed!</h4>
                                        <p>{{serverErrorMessage}}</p>
                                    </div>
                                    <form role="form" id="newItineraryForm">
                                        <div class="box-body">
                                            <div class="overlay" ng-show="saving == true">
                                                <i class="fa fa-refresh fa-spin"></i>
                                            </div>
                                            <div class="nav-tabs-custom">
                                                <ul class="nav nav-tabs">
                                                    <li class="active">
                                                        <a href="#itineraryTab" data-toggle="tab">Basic</a>
                                                    </li>
                                                    <li>
                                                        <a href="#countriesTab" data-toggle="tab">Countries</a>
                                                    </li>
                                                    <li>
                                                        <a href="#ferriesTab" data-toggle="tab">Ferries</a>
                                                    </li>
                                                    <li>
                                                        <a href="#flightsTab" data-toggle="tab">Flights</a>
                                                    </li>
                                                    <li>
                                                        <a href="#hotelsTab" data-toggle="tab">Hotels</a>
                                                    </li>
                                                    <li>
                                                        <a href="#seasonsTab" data-toggle="tab">Seasons</a>
                                                    </li>
                                                    <li>
                                                        <a href="#budgetTab" data-toggle="tab">Budget</a>
                                                    </li>
                                                    <li>
                                                        <a href="#purposeTab" data-toggle="tab">Purpose</a>
                                                    </li>
                                                </ul>
                                                <div class="tab-content">
                                                    <div class="tab-pane active" id="itineraryTab">
                                                        <div class="form-group" ng-show="saving==false">
                                                            <label class="control-label text-red" for="en_name" ng-show="en_nameMissing">
                                                                <i class="fa fa-times-circle-o"></i>English Name is missing</label>
                                                            <label for="en_name" ng-show="!en_nameMissing">En Name</label>
                                                            <input type="text" required class="form-control" id="en_name" ng-model="newItinerary.en_name" placeholder="Enter name in English"
                                                                required>
                                                        </div>
                                                        <div class="form-group" ng-show="saving==false">
                                                            <label class="control-label text-red" for="en_description" ng-show="en_descriptionMissing">
                                                                <i class="fa fa-times-circle-o"></i>English Description is missing</label>
                                                            <label for="en_description" ng-show="!en_descriptionMissing">En Desc</label>
                                                            <input type="text" required class="form-control" id="en_description" ng-model="newItinerary.en_description" placeholder="Enter description in English"
                                                                required>
                                                        </div>
                                                        <div class="form-group" ng-show="saving==false">
                                                            <label class="control-label text-red" for="ar_name" ng-show="ar_nameMissing">
                                                                <i class="fa fa-times-circle-o"></i>Arabic Name is missing</label>
                                                            <label for="ar_name" ng-show="!ar_nameMissing">Ar Name</label>
                                                            <input type="text" required class="form-control" id="ar_name" ng-model="newItinerary.ar_name" placeholder="Enter name in Arabic"
                                                                required>
                                                        </div>
                                                        <div class="form-group" ng-show="saving==false">
                                                            <label class="control-label text-red" for="ar_description" ng-show="ar_descriptionMissing">
                                                                <i class="fa fa-times-circle-o"></i>Arabic Description is missing</label>
                                                            <label for="ar_description" ng-show="!ar_descriptionMissing">Ar Desc</label>
                                                            <input type="text" required class="form-control" id="ar_description" ng-model="newItinerary.ar_description" placeholder="Enter description in Arabic"
                                                                required>
                                                        </div>
                                                        <div class="form-group" ng-show="saving==false">
                                                            <label class="control-label text-red" for="dailySpendings" ng-show="dailySpendingsMissing">
                                                                <i class="fa fa-times-circle-o"></i>Daily Spendings is missing</label>
                                                            <label for="dailySpendings" ng-show="!dailySpendingsMissing">Daily Spendings</label>
                                                            <input type="text" required class="form-control" id="dailySpendings" ng-model="newItinerary.dailySpendings" placeholder="Enter daily spendings"
                                                                required>
                                                        </div>
                                                        <!-- /.form group -->
                                                    </div>
                                                    <!-- /.tab-pane -->
                                                    <div class="tab-pane" id="countriesTab">
                                                        <select class="form-control" style="height: 300px" multiple ng-model="newItinerary.countries">
                                                            <option ng-repeat="country in countries" ng-value="country.id">{{country.en_name}}</option>
                                                        </select>
                                                        <!-- /.form group -->
                                                    </div>
                                                    <!-- /.tab-pane -->
                                                    <style>
                                                        .form-control-2 {
                                                            width: 16%;
                                                            display: inline-block;
                                                        }

                                                        .form-control-4 {
                                                            width: 33%;
                                                            display: inline-block;
                                                        }

                                                        .form-control-6 {
                                                            width: 49.5%;
                                                            display: inline-block;
                                                        }
                                                    </style>
                                                    <div class="tab-pane" id="ferriesTab">
                                                        <div class="text-center">
                                                            <i class="fa fa-plus-circle" style="font-size: 30px" aria-hidden="true" ng-click="addFerry()"></i>
                                                        </div>
                                                        <div class="box box-primary" ng-repeat="ferry in newItinerary.ferries">
                                                            <div class="box-header with-border">
                                                                <h3 class="box-title">Title</h3>
                                                                <div class="box-tools pull-right">
                                                                    <button class="btn btn-box-tool" ng-click="removeFerry($index)">
                                                                        <i class="fa fa-times-circle text-danger"></i>
                                                                    </button>
                                                                    <button class="btn btn-box-tool" data-widget="collapse">
                                                                        <i class="fa fa-plus"></i>
                                                                    </button>
                                                                </div>
                                                                <!-- /.box-tools -->
                                                            </div>
                                                            <!-- /.box-header -->
                                                            <div class="box-body">
                                                                <div class="form-group" style="border: 1px solid #eee;padding: 15px;">
                                                                    <div class="form-control-6">
                                                                        <label>Departing From:</label>
                                                                        <input class="form-control" type="text" ng-model="ferry.departing_from" required>
                                                                    </div>
                                                                    <div class="form-control-6">
                                                                        <label>Arriving To:</label>
                                                                        <input class="form-control" type="text" ng-model="ferry.arriving_to" required>
                                                                    </div>
                                                                    <label>Carrier Name:</label>
                                                                    <input class="form-control" type="text" ng-model="ferry.carrier_name" required>
                                                                    <label>Price:</label>
                                                                    <br>
                                                                    <div class="form-control-2">
                                                                        <label>Jan:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.JAN" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Feb:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.FEB" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Mar:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.MAR" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Apr:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.APR" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>May:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.MAY" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Jun:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.JUN" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Jul:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.JUL" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Aug:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.AUG" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Sept:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.SEP" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Oct:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.OCT" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Nov:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.NOV" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Dec:</label>
                                                                        <input class="form-control" type="number" ng-model="ferry.DEC" required>
                                                                    </div>
                                                                </div>
                                                                <!-- /.form group -->
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- /.tab-pane -->
                                                    <div class="tab-pane" id="flightsTab">
                                                        <div class="text-center">
                                                            <i class="fa fa-plus-circle" style="font-size: 30px" aria-hidden="true" ng-click="addFlight()"></i>
                                                        </div>
                                                        <div class="box box-primary" ng-repeat="flight in newItinerary.flights">
                                                            <div class="box-header with-border">
                                                                <h3 class="box-title">Title</h3>
                                                                <div class="box-tools pull-right">
                                                                    <button class="btn btn-box-tool" ng-click="removeFlight($index)">
                                                                        <i class="fa fa-times-circle text-danger"></i>
                                                                    </button>
                                                                    <button class="btn btn-box-tool" data-widget="collapse">
                                                                        <i class="fa fa-plus"></i>
                                                                    </button>
                                                                </div>
                                                                <!-- /.box-tools -->
                                                            </div>
                                                            <!-- /.box-header -->
                                                            <div class="box-body">
                                                                <div class="form-group" style="border: 1px solid #eee;padding: 15px;">
                                                                    <div class="form-control-6">
                                                                        <label>Departing From:</label>
                                                                        <input class="form-control" type="text" ng-model="flight.departing_from" required>
                                                                    </div>
                                                                    <div class="form-control-6">
                                                                        <label>Arriving To:</label>
                                                                        <input class="form-control" type="text" ng-model="flight.arriving_to" required>
                                                                    </div>
                                                                    <div class="form-control-6">
                                                                        <label>Airline:</label>
                                                                        <input class="form-control" type="text" ng-model="flight.airline_name" required>
                                                                    </div>
                                                                    <div class="form-control-6">
                                                                        <label>Type:</label>
                                                                        <select class="form-control" ng-model="flight.type">
                                                                            <option value="0" ng-selected="flight.type == 0">External</option>
                                                                            <option value="1" ng-selected="flight.type == 1">Internal</option>
                                                                        </select>
                                                                    </div>
                                                                    <label>Adult Price:</label>
                                                                    <br>
                                                                    <div class="form-control-2">
                                                                        <label>Jan:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aJAN" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Feb:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aFEB" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Mar:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aMAR" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Apr:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aAPR" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>May:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aMAY" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Jun:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aJUN" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Jul:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aJUL" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Aug:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aAUG" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Sept:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aSEP" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Oct:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aOCT" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Nov:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aNOV" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Dec:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.aDEC" required>
                                                                    </div>
                                                                    <label>Kid Price:</label>
                                                                    <br>
                                                                    <div class="form-control-2">
                                                                        <label>Jan:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.kJAN" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Feb:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.kFEB" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Mar:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.kMAR" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Apr:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.kAPR" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>May:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.kMAY" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Jun:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.kJUN" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Jul:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.kJUL" required>
                                                                    </div>
                                                                    <div class="form-control-2">
                                                                        <label>Aug:</label>
                                                                        <input class="form-control" type="number" ng-model="flight.kAUG" required>
                                                                    </div>
