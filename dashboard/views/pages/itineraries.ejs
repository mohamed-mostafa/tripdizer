<div class="box">
    <div class="box-header">
        <h3 class="box-title">
            <%= title %>
        </h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body">
        <div class="overlay" ng-show="loading == true">
            <i class="fa fa-refresh fa-spin"></i>
        </div>
        <div class="box-body table-responsive no-padding" ng-show="loading == false">
            <table id="itinerariesTable" class="table table-bordered table-striped">
                <thead>
                    <tr>
                        <th>Name</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="itinerary in itineraries">
                        <td>
                            <a ng-click="openDetailsDialog(itinerary.id)" href="#">{{itinerary.en_name}}</a>
                        </td>
                    </tr>
                </tbody>
                <tfooter>
                    <tr>
                        <td colspan="7">
                            <button type="button" class="btn btn-primary pull-right" data-toggle="modal" data-target="addItineraryModal" ng-click="openAddDialog()">Add Itinerary</button>
                        </td>
                    </tr>
                </tfooter>
            </table>
        </div>
    </div>
    <!-- /.box-body -->
</div>
<!-- /.box -->
<!-- Main row -->
<div class="row">

    <!-- MODALS -->

    <div class="modal fade" id="addItineraryModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h4 class="modal-title" id="myModalLabel">Add or Modify Itinerary</h4>
                </div>
                <div class="modal-body">
                    <div class="callout callout-danger" ng-show="serverError==true">
                        <h4>Saving failed!</h4>
                        <p>{{serverErrorMessage}}</p>
                    </div>
                    <form role="form" id="newItineraryForm">
                        <div class="box-body">
                            <div class="overlay" ng-show="saving == true">
                                <i class="fa fa-refresh fa-spin"></i>
                            </div>
                            <div class="form-group form-group-6" ng-show="saving==false">
                                <label class="control-label text-red" for="en_name" ng-show="en_nameMissing==true">
                                    <i class="fa fa-times-circle-o"></i>English Name is missing</label>
                                <label for="en_name" ng-show="en_nameMissing==false">En Name</label>
                                <input type="text" required class="form-control" id="en_name" ng-model="newItinerary.en_name" placeholder="Enter name in English"
                                    required>
                            </div>
                            <div class="form-group form-group-6" ng-show="saving==false">
                                <label class="control-label text-red" for="ar_name" ng-show="ar_nameMissing==true">
