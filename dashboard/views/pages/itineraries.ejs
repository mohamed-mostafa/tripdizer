                                <label class="control-label text-red" for="needsVisa" ng-show="dailySpendingsMissing==true">
                                    <i class="fa fa-times-circle-o"></i>Need a visa is missing</label>
                                <label for="needsVisa" ng-show="needsVisaMissing==false">Need a visa</label>
                                <select class="form-control" ng-model="newItinerary.needsVisa">
                                    <option value="1" ng-selected="true">Yes</option>
                                    <option value="0">No</option>
                                </select>
                            </div>
                            <div class="form-group form-group-7" ng-show="saving==false">
                                <label class="control-label text-red" for="image1" ng-show="image1Missing==true">
                                    <i class="fa fa-times-circle-o"></i>Image is missing</label>
                                <label for="image1" ng-show="image1Missing==false">Image URI (recommended width 600px)</label>
                                <input type="url" class="form-control" id="image1" ng-model="newItinerary.image1" placeholder="Enter image URI" required>
                            </div>
                            <!-- <div class="form-group form-group-6" ng-show="saving==false">
                                                <label class="control-label text-red" for="image2" ng-show="image2Missing==true">
                                                    <i class="fa fa-times-circle-o"></i>Image 2 is missing</label>
                                                <label for="image2" ng-show="image2Missing==false">Image 2 URI</label>
                                                <input type="text" required class="form-control" id="image2" ng-model="newItinerary.image2" placeholder="Enter image 2 URI"
                                                    required>
                                            </div> -->
