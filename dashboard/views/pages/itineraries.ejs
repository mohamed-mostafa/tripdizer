                                <label for="dailySpendings" ng-show="dailySpendingsMissing==false">Daily Spendings</label>
                                <input type="number" required class="form-control" id="dailySpendings" ng-model="newItinerary.dailySpendings" placeholder="Enter daily spendings"
                                    required>
                            </div>
