                                <label for="includes" ng-show="includesMissing==false">Includes</label>
                                <textarea bullet class="form-control" id="includes" ng-model="newItinerary.includes" placeholder="Enter includes" rows="2"></textarea>
                            </div>
                            <div class="form-group" ng-show="saving==false">
